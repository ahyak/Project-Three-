$.ajax({
  url: '/api/locations'
})
  .done(function(locations) {
    console.log(locations)
    var continentsDataProvider;

    // Define SVG path for target icon
    // var targetSVG =
    // "M9,0C4.029,0,0,4.029,0,9s4.029,9,9,9s9-4.029,9-9S13.971,0,9,0z M9,15.93 c-3.83,0-6.93-3.1-6.93-6.93S5.17,2.07,9,2.07s6.93,3.1,6.93,6.93S12.83,15.93,9,15.93 M12.5,9c0,1.933-1.567,3.5-3.5,3.5S5.5,10.933,5.5,9S7.067,5.5,9,5.5 S12.5,7.067,12.5,9z";

    var worldDataProvider = {
      "map": "worldLow",
      "getAreasFromMap": true,
      "images": locations,
      // "images": [ {
      //   "rollOverColor": "#9a7bca",
      //   "labelRollOverColor": "#9a7bca",
      //   "useTargetsZoomValues": true,
      //   "left": 30,
      //   "bottom": 30,
      //   "labelFontSize": 15,
      //   "selectable": true
      // }, {
      // "svgPath": targetSVG,
      // "zoomLevel": 5,
      // "scale": 0.5,
      // "title": "Kinshasa",
      // "latitude": -4.3369,
      // "longitude": 15.3271
      //
      // } ]
    };

    var continentsDataProvider = {
      "map": "continentsLow",

      "areas": [ {
        "id": "africa",
        "linkToObject": worldDataProvider,
        "color": "#605675",
        "passZoomValuesToTarget": true
      }, {
        "id": "asia",
        "linkToObject": worldDataProvider,
        "color": "#a791b4",
        "passZoomValuesToTarget": true
      }, {
        "id": "australia",
        "linkToObject": worldDataProvider,
        "color": "#7f7891",
        "passZoomValuesToTarget": true
      }, {
        "id": "europe",
        "linkToObject": worldDataProvider,
        "color": "#9186a2",
        "passZoomValuesToTarget": true,
      }, {
        "id": "north_america",
        "linkToObject": worldDataProvider,
        "color": "#868191",
        "passZoomValuesToTarget": true
      }, {
        "id": "south_america",
        "linkToObject": worldDataProvider,
        "color": "#8f7ea9",
        "passZoomValuesToTarget": true
      } ]
    };

    var map = AmCharts.makeChart( "mapdiv", {
      "type": "map",
      "theme": "light",
      "projection": "miller",

      "areasSettings": {
        "autoZoom": true,
        "rollOverOutlineColor": "#9a7bca",
        "selectedColor": "#9a7bca",
        "color": "#a791b4",
        "rollOverColor": "#9a7bca",
        // marker areas settings
        "unlistedAreasColor": "#15A892"
      },

      "zoomControl": {
        "homeIconFile": "ammap/images/homeIcon.gif",
        "buttonFillColor": "#fff",
      },

      "imagesSettings": {
        "rollOverColor": "#089282",
        "rollOverScale": 3,
        "selectedScale": 3,
        "selectedColor": "#089282",
        "color": "#13564e"
      },

      "dataProvider": continentsDataProvider,


      "listeners": {
        "event": "homeButtonClicked",
        "method": handleGoHome
      },

    });

    function handleGoHome() {
      map.dataProvider = continentsDataProvider;
      map.validateNow();
    }
  })
